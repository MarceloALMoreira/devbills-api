//Configurações do TSC CONFI

{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,

    //Config para build
    "resolveJsonModule": true,
    "outDir": "./build"
  }
}
